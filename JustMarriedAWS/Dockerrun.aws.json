{
  "AWSEBDockerrunVersion": "2",
  "volumes": [
    {
      "name": "db",
      "host": {
        "sourcePath": "/db"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "memcached",
      "image": "memcached:1.4.36",
      "essential": true,
      "memory": 128
    },
    {
      "name": "db",
      "image": "mongo:3.4.3",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 27017,
          "containerPort": 27017
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "db",
          "containerPath": "/db"
        }
      ]
    },
    {
      "name": "jmserver",
      "image": "justmarried/just-married-server:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 2701,
          "containerPort": 2701
        }
      ],
      "links": [
        "db", "memcached"
      ],
      "environment": [
        {
          "name": "SESSION_SECRET",
          "value": "jlaj3oiajrfhfwaosifhasfafag"
        },
        {
          "name": "MEMCACHED_SERVERS",
          "value": "memcached"
        },
        {
          "name": "MEMCACHED_USERNAME",
          "value": "memcached"
        },
        {
          "name": "MEMCACHED_PASSWORD",
          "value": ""
        },
        {
          "name": "HOST",
          "value": ""
        },
        {
          "name": "PORT",
          "value": ""
        },
        {
          "name": "DOMAIN",
          "value": ""
        },
        {
          "name": "DB_URL",
          "value": "db"
        },
        {
          "name": "JWT_SECRET",
          "value": "asfwu082uwroidfosf"
        }
      ]
    },
    {
      "name": "jmclient",
      "image": "justmarried/just-married-client:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3000
        }
      ],
      "links": [
        "jmserver"
      ]
    }
  ]
}